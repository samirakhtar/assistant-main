{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist",
    "module": "CommonJS",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "exclude": ["node_modules"]
}
